<beans>
    <bean id="PropertyNormalizer"
          class="org.openconfig.core.bean.LowercasePropertyNormalizer"/>
    <bean id="OpenConfigContext"
          class="org.openconfig.core.BasicOpenConfigContext"/>
    <bean id="MutableConfigurator"
          class="org.openconfig.configurators.MapConfigurator"/>
    <bean id="ConfiguratorFactory"
          class="org.openconfig.factory.impl.ConfiguratorFactoryImpl"/>
    <bean id="EnvironmentResolver"
          class="org.openconfig.core.SystemPropertyEnvironmentResolver"/>
</beans>